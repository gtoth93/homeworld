            IFONCE ( TimerExpiredDestroy( "PingHeadshot" ) )
                PingAddShips( TEAMSHIPS_Headshot, "HeadshotAsteroidPing");
                TutSetPointerTargetShipSelection( "HeadshotPointer", TEAMSHIPS_Headshot );
                VarSet("PingedHeadshot", TRUE);
            ENDIFONCE
        ENDWATCH
    ENDSTATE FIIntro2


        WATCH
            
            IF ( TimerRemaining ("LetterboxInTimer") = 2 )
                OpenSensors( 120000 );
                PauseUniverse();
            ENDIF

            
            IF ( TimerExpiredDestroy( "LetterboxInTimer" ) )
                JUMP FIIntro2;
            ENDIF
        ENDWATCH
    ENDSTATE FIIntroLBXIn

    
    
    
    
    
    STATE FIIntro2
        INIT
            SpeechEvent((704 + 4096 + 268435456), 0);
            TimerCreateSetStart ( "PingHeadshot", 1 );
            VarCreateSet("PingedHeadshot", FALSE);

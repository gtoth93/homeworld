          VarDestroy("G_CLICK_DestroyUnknownVessel");
          VarDestroy("G_CLICK_SalvageUnknownVessel");
        ENDINIT
        WATCH
          IFONCE (MsgReceived("WereAttacking"))
            JUMP FIDangerWillRobinson;
          ENDIFONCE

          
          IFONCE (MsgReceived("FirstShipUsurped"))

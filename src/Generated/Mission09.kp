              TeamAttributesBitSet(0x3000);
              TeamSwitchPlayerOwner();
            ENDINIT
            WATCH
              IFONCE (TimerExpiredDestroy("MissileTimer"))
                VarCreateSet("G_GiveMissileTech",TRUE);
              ENDIFONCE
              IF (VarGet("G_ItsAlive")=1)
                JUMP Waiting;
              ENDIF
            ENDWATCH
        ENDSTATE Dormant 

